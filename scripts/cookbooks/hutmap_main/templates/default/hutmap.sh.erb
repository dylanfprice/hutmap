<% node.each do |key, value| %>
  <% if key.to_s.start_with?('HUTMAP') %>
export <%= key %>='<%= value %>'  
  <% end %>
<% end %>
export HUTMAP_CLOSURE_COMPILER='<%= node[:install_dir] %>/closure-compiler/compiler.jar'
export HUTMAP_CLOSURE_TEMPLATES='<%= node[:install_dir] %>/closure-templates/SoyToJsSrcCompiler.jar'
export HUTMAP_DEV_PATH='<%= node[:dev_path] %>'

export PATH=$PATH:'<%= node[:install_dir] %>/node/bin/'
